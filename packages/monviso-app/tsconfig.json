{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "./lib",
    "resolveJsonModule": true,
    "allowJs": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "noEmit": true,
    "isolatedModules": true
  },
  "include": [
    "./src"
  ],
  "typedocOptions": {
    "mode": "modules",
    "out": "docs"
  }
}
